<div class="container">
	<div class="row">
		<ol class="breadcrumb">
			<li>
				<a href="{{assets}}/index-{{language}}.html">{{#i18n "tmpl-home"}}{{/i18n}}</a>
			</li>
{{#isnt assets ".."}}
			<li>
				<a href="{{assets}}/{{lowercase category}}s/index-{{language}}.html">{{category}}s</a>
			</li>
{{/isnt}}
{{#is breadcrumb3 "true"}}
			<li>
				<a href="{{assets}}/{{lowercase category}}s/{{tag}}/index-{{language}}.html">{{breadcrumb3-title}}</a>
			</li>
{{/is}}
			<li>{{title}}</li>
		</ol>
	</div>
</div>
