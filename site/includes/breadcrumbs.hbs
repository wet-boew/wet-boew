<div class="container">
	<div class="row">
		<ol class="breadcrumb">
			<li>
				<a href="{{assets}}/index-{{language}}.html">{{{i18n "tmpl-home"}}}</a>
			</li>
{{#contains page.src "/demos/index"}}
{{else}}
	{{#contains page.src "/docs/"}}
		{{#contains page.src "/versions/"}}
			{{#contains page.src "/index"}}
			{{else}}
				<li>
					<a href="{{assets}}/docs/versions/index-{{language}}.html">{{{i18n "tmpl-version-history"}}}</a>
				</li>
			{{/contains}}
		{{else}}
			{{#contains page.src "/docs/index"}}
			{{else}}
					<li>
						<a href="{{assets}}/docs/index-{{language}}.html">{{{i18n "tmpl-documentation"}}}</a>
					</li>
				{{#contains page.src "/gs-cd/"}}
					{{#contains page.src "/index"}}
					{{else}}
					<li>
						<a href="{{assets}}/docs/gs-cd/index-{{language}}.html">{{{i18n "tmpl-getting-started"}}}</a>
					</li>
					{{/contains}}
				{{/contains}}
				{{#if page.componentType}}
					<li>
						<a href="{{assets}}/docs/{{page.componentType}}-{{language}}.html">{{#i18n "tmpl-#{page.componentType}"}}{{/i18n}}</a>
					</li>
				{{/if}}
			{{/contains}}
		{{/contains}}
	{{else}}
			<li>
				<a href="{{assets}}/demos/index-{{language}}.html">{{{i18n "tmpl-work-examples"}}}</a>
			</li>
		{{#is breadcrumb3 "true"}}
			{{#contains tag "theme-wet-boew"}}
				<li>
					<a href="{{assets}}/theme/index-{{language}}.html">{{breadcrumb3-title}}</a>
				</li>
			{{else}}
				<li>
					<a href="{{assets}}/demos/{{tag}}/index-{{language}}.html">{{breadcrumb3-title}}</a>
				</li>
			{{/contains}}
		{{/is}}
	{{/contains}}
{{/contains}}
			<li>{{title}}</li>
		</ol>
	</div>
</div>
