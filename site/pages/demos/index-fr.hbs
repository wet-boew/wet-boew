---
{
	"title": "Exemples pratiques",
	"language": "fr",
	"en": "index"
}
---
<table id="components" class="wb-prettify wb-tables table table-striped table-hover wb-inview" data-wet-boew='{"aoColumnDefs": [ { "bVisible": false, "aTargets": [ 3 ] } ], "aLengthMenu": [[10, 25, -1], [10, 25, "All"]], "iDisplayLength": -1 }'>
	<thead>
		<tr>
			<th>Nom</th>
			<th>Catégorie</th>
			<th>Description</th>
			<th>Mots clés</th>
		</tr>
	</thead>
	<tbody>
		{{#withSort pages "data.title"}}
			{{#is data.language "fr"}}
				{{#if data.category}}
					<tr>
						<td><a href="{{relative dirname dest}}{{#if data.querystring}}?{{data.querystring}}{{/if}}">{{data.title}}</a></td>
						<td>{{data.category}}</td>
						<td>{{data.description}}</td>
						<td>{{data.tag}}</td>
					</tr>
				{{/if}}
			{{/is}}
		{{/withSort}}
	</tbody>
</table>
