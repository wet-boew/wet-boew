{
	"title": "Exemples pratiques",
	"language": "fr",
	"altLangPrefix": "index",
	"dateModified": "2014-07-07",
	"themes": [
		{
			"tag": "theme-wet-boew",
			"title": "Thème de la Boîte à outils de l'expérience Web (BOEW)",
			"description": "Un exemple de thème conçu à partir du thème de base.",
			"href": "../theme/index-fr.html"
		},
		{
			"tag": "theme-gcwu-fegc",
			"title": "Thème de la facilité d’emploi Web du gouvernement du Canada",
			"description": "Ce thème met en place la mise en page et la mise en forme exigés par la <a href='https://www.tbs-sct.gc.ca/pol/doc-eng.aspx?id=24227' rel='external'>Norme sur la facilité des sites Web</a>. Il est recommandé de se servir de ce thème sur les sites Web du gouvernement du Canada (sauf Canada.ca).",
			"href": "https://wet-boew.github.io/themes-dist/theme-gcwu-fegc/index-fr.html"
		},
		{
			"tag": "theme-gc-intranet",
			"title": "Thème du gouvernement du Canada pour les sites intranet",
			"description": "Une version du thème de la facilité d'emploi des sites Web du gouvernement du Canada pour les sites intranet.",
			"href": "https://wet-boew.github.io/themes-dist/theme-gc-intranet/index-fr.html"
		},
		{
			"tag": "GCWeb",
			"title": "Thème Canada.ca",
			"description": "Ce thème met en place la mise en page et la mise en forme exigés pour le site Canada.ca.",
			"href": "https://wet-boew.github.io/themes-dist/GCWeb/index-fr.html"
		},
		{
			"tag": "theme-ogpl",
			"title": "Thème de la Plate-forme de gouvernement ouvert (PGO)",
			"description": "Un thème qui met en place la mise en page et la mise en forme pour la <a href='https://ogpl.github.io/index-fr.html' rel='external'>Plate-forme de gouvernement ouvert (PGO)</a>.",
			"href": "https://wet-boew.github.io/themes-dist/theme-ogpl/index-fr.html"
		},
		{
			"tag": "theme-base",
			"title": "Thème de base",
			"description": "Un modèle de thème pour la création de nouveaux thèmes.",
			"href": "https://wet-boew.github.io/themes-dist/theme-base/index-fr.html"
		}
	],
	"variants": [
		{
			"tag": "jekyll",
			"title": "Variant pour Jekyll",
			"description": "Adapte la BOEW pour l’utilisation avec Jekyll.",
			"href": "https://wet-boew.github.io/wet-boew-jekyll/index-fr.html"
		}
	]
