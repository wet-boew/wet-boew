---
{
	"title": "Exemples pratiques",
	"language": "fr",
	"en": "index"
}
---
<style>
#components td, #components th {
	vertical-align: top;
	padding: 10px;
}
#components th {
	padding-right: 20px;
}
#components tbody tr:nth-child(odd) {
	background: #F0FCFF;
}
</style>
<table id="components" class="wb-prettify wb-tables" data-wet-boew='{"aoColumnDefs": [ { "bVisible": false, "aTargets": [ 3 ] } ], "aLengthMenu": [[10, 25, -1], [10, 25, "All"]], "iDisplayLength": -1, "aaSorting": [[ 1, "asc" ]]}'>
	<thead>
		<tr>
			<th>Nom</th>
			<th>Catégorie</th>
			<th>Description</th>
			<th class="wb-inv">Mots clés</th>
		</tr>
	</thead>
	<tbody>
		{{#each pages}}
			{{#is data.language "fr"}}
				{{#if data.category}}
					<tr>
						<td><a href="{{relative dirname dest}}">{{data.title}}</a></td>
						<td>{{data.category}}</td>
						<td>{{data.description}}</td>
						<td>{{data.tag}}</td>
					</tr>
				{{/if}}
			{{/is}}
		{{/each}}
	</tbody>
</table>
